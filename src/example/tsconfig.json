{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "Node",
    "types": ["@citizenfx/server", "@citizenfx/client", "node", "reflect-metadata"],
    "resolveJsonModule": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.promise"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
